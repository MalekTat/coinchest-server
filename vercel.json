{
    "version": 2,
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OTIONS"],
        "headers": {
          "Access-Control-Allow_Origin": "*"
        }
      }
    ]
  }