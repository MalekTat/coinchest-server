{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@MONGODB_URI",
    "ORIGIN": "@ORIGIN",
    "TOKEN_SECRET" : "@TOKEN_SECRET",
    "CLOUD_NAME" : "@CLOUD_NAME",
    "CLOUD_API_KEY" : "@CLOUD_API_KEY", 
    "CLOUD_API_SECRET" : "@CLOUD_API_SECRET",
    "COINGECKO_API_KEY" : "@COINGECKO_API_KEY"
  }
}